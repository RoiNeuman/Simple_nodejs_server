{
  "name": "simple_nodejs_server",
  "version": "1.2.1",
  "description": "Simple website based on NodeJS server with Express framework and React as frontend with Redux and Webpack",
  "main": "Server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node index.js",
    "serve": "nodemon dist/app/server.js --ignore components",
    "webpack": "webpack --config webpack.config.js",
    "babel-w": "babel src -d ./dist -w -s --ignore src/content/",
    "build": "babel src -d ./dist -s --ignore src/content/ | npm run webpack"
  },
